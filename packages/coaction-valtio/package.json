{
  "name": "@coaction/valtio",
  "version": "0.1.5",
  "description": "A Coaction integration tool for Valtio",
  "keywords": [
    "state",
    "coaction",
    "valtio",
    "proxy"
  ],
  "authors": [
    "Michael Lin <unadlib@gmail.com> (https://github.com/unadlib)"
  ],
  "homepage": "https://github.com/unadlib/coaction/tree/main/packages/coaction-valtio#readme",
  "license": "MIT",
  "main": "dist/coaction-valtio.cjs.js",
  "module": "dist/coaction-valtio.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/coaction-valtio.cjs.mjs",
        "default": "./dist/coaction-valtio.cjs.js"
      },
      "module": "./dist/coaction-valtio.esm.js",
      "import": "./dist/coaction-valtio.cjs.mjs",
      "default": "./dist/coaction-valtio.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/coaction-valtio.cjs.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/unadlib/coaction.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "vitest run test"
  },
  "bugs": {
    "url": "https://github.com/unadlib/coaction/issues"
  },
  "preconstruct": {
    "umdName": "CoactionValtio",
    "entrypoints": [
      "./index.ts"
    ]
  },
  "peerDependencies": {
    "coaction": "^0.1.5",
    "mutative": "^1.1.0",
    "valtio": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "coaction": {
      "optional": true
    },
    "mutative": {
      "optional": true
    },
    "valtio": {
      "optional": true
    }
  },
  "devDependencies": {
    "coaction": "^0.1.5",
    "mutative": "^1.3.0",
    "valtio": "^2.1.9"
  },
  "dependencies": {
    "mutability": "^1.2.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
