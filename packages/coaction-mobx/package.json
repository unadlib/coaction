{
  "name": "@coaction/mobx",
  "version": "0.1.5",
  "description": "A Coaction integration tool for MobX",
  "keywords": [
    "state",
    "coaction",
    "mobx"
  ],
  "authors": [
    "Michael Lin <unadlib@gmail.com> (https://github.com/unadlib)"
  ],
  "homepage": "https://github.com/unadlib/coaction/tree/main/packages/coaction-mobx#readme",
  "license": "MIT",
  "main": "dist/coaction-mobx.cjs.js",
  "module": "dist/coaction-mobx.esm.js",
  "umd:main": "dist/coaction-mobx.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/coaction-mobx.cjs.mjs",
        "default": "./dist/coaction-mobx.cjs.js"
      },
      "module": "./dist/coaction-mobx.esm.js",
      "import": "./dist/coaction-mobx.cjs.mjs",
      "default": "./dist/coaction-mobx.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/coaction-mobx.cjs.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/unadlib/coaction.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "vitest run test"
  },
  "bugs": {
    "url": "https://github.com/unadlib/coaction/issues"
  },
  "preconstruct": {
    "umdName": "CoactionMobx",
    "entrypoints": [
      "./index.ts"
    ]
  },
  "peerDependencies": {
    "coaction": "^0.1.5",
    "mobx": "^6.13.2",
    "mutative": "^1.1.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "coaction": {
      "optional": true
    },
    "mobx": {
      "optional": true
    },
    "mutative": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/use-sync-external-store": "^1.5.0",
    "coaction": "^0.1.5",
    "data-transport": "^5.0.3",
    "jsdom": "^28.1.0",
    "jsdom-global": "^3.0.2",
    "mobx": "^6.15.0",
    "mutative": "^1.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "dependencies": {
    "mutability": "^1.2.1"
  }
}
