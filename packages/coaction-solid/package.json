{
  "name": "@coaction/solid",
  "version": "0.0.1",
  "description": "Coaction tools for SolidJS",
  "keywords": [
    "state",
    "coaction",
    "solidjs"
  ],
  "authors": [
    "Michael Lin <unadlib@gmail.com> (https://github.com/unadlib)"
  ],
  "homepage": "https://github.com/unadlib/coaction/tree/main/packages/coaction-solid#readme",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/coaction-solid.cjs.js",
  "module": "dist/coaction-solid.esm.js",
  "umd:main": "dist/coaction-solid.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/coaction-solid.cjs.mjs",
        "default": "./dist/coaction-solid.cjs.js"
      },
      "module": "./dist/coaction-solid.esm.js",
      "import": "./dist/coaction-solid.cjs.mjs",
      "default": "./dist/coaction-solid.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/unadlib/coaction.git"
  },
  "scripts": {},
  "bugs": {
    "url": "https://github.com/unadlib/coaction/issues"
  },
  "preconstruct": {
    "umdName": "CoactionSolid",
    "entrypoints": [
      "./index.ts"
    ]
  },
  "peerDependencies": {
    "coaction": "^0.0.1",
    "solid-js": "^1.6.0"
  },
  "peerDependenciesMeta": {
    "coaction": {
      "optional": true
    }
  },
  "devDependencies": {
    "coaction": "^0.0.1",
    "solid-js": "^1.7.6",
    "solid-testing-library": "^0.3.0"
  }
}
