{
  "name": "@coaction/xstate",
  "version": "1.0.0",
  "description": "A Coaction integration tool for XState",
  "keywords": [
    "state",
    "coaction",
    "xstate",
    "actor"
  ],
  "authors": [
    "Michael Lin <unadlib@gmail.com> (https://github.com/unadlib)"
  ],
  "homepage": "https://github.com/unadlib/coaction/tree/main/packages/coaction-xstate#readme",
  "license": "MIT",
  "main": "dist/coaction-xstate.cjs.js",
  "module": "dist/coaction-xstate.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/coaction-xstate.cjs.mjs",
        "default": "./dist/coaction-xstate.cjs.js"
      },
      "module": "./dist/coaction-xstate.esm.js",
      "import": "./dist/coaction-xstate.cjs.mjs",
      "default": "./dist/coaction-xstate.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/coaction-xstate.cjs.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unadlib/coaction.git",
    "directory": "packages/coaction-xstate"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "vitest run test"
  },
  "bugs": {
    "url": "https://github.com/unadlib/coaction/issues"
  },
  "preconstruct": {
    "umdName": "CoactionXState",
    "entrypoints": [
      "./index.ts"
    ]
  },
  "peerDependencies": {
    "coaction": "^0.2.0",
    "xstate": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "coaction": {
      "optional": true
    },
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "coaction": "^0.2.0",
    "xstate": "^5.23.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "author": "unadlib"
}
